programa {
  funcao inicio() {
    
    inteiro carrosonho, carroeconomico, orcamento

    carrosonho = 25000
    carroeconomico = 15000
    orcamento = 20000

    se (orcamento > carrosonho)
      escreva ("Você pode comprar o carro dos seus sonhos!")
    senao
      escreva ("Você precisa optar por um carro econômico.")
  }
}
